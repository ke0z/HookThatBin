        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HookAMal"
        mc:Ignorable="d"
        ResizeMode="CanMinimize"
        Title="SpawnWindow" Height="250" Width="400">
    <Grid>
        <Label Content="Program" HorizontalAlignment="Left" Margin="28,33,0,0" VerticalAlignment="Top"/>
        <Label Content="Args" HorizontalAlignment="Left" Margin="28,64,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Margin="89,33,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="270" Height="26"/>
        <Grid>
            <Button Content="+" HorizontalAlignment="Left" Margin="316,79,0,0" VerticalAlignment="Top" RenderTransformOrigin="6.248,0.309" Click="Button_Click"/>
            <Button Content="-" HorizontalAlignment="Left" Margin="345,79,0,0" VerticalAlignment="Top" RenderTransformOrigin="6.248,0.309"/>
        </Grid>

    </Grid>
</Window>
